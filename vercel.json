{
  "version": 2,
  "buildCommand": "cd apps/web && bun install && bunx prisma generate && bun run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "bun install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/check-missed",
      "schedule": "* * * * *"
    }
  ]
}

